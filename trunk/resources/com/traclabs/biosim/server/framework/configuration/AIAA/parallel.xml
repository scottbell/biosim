<?xml version="1.0" encoding="UTF-8"?>
<biosim xmlns:p="http://www.traclabs.com/biosim" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.traclabs.com/biosim ../../schema/BiosimInitSchema.xsd ">
  <Globals crewsToWatch="" runTillCrewDeath="true">
  	<author>Haibei and Francisco</author>
  </Globals>
  <SimBioModules>
  	<air>
  		<CO2Store capacity="10000" moduleName="CO2Store" level="0">
  			<expFailureDecider lambda="0.00001447" ></expFailureDecider></CO2Store>
  		<H2Store capacity="10000" moduleName="H2Store" level="0">
  			<expFailureDecider lambda="0.00001447" ></expFailureDecider></H2Store>
  		<O2Store capacity="10000" moduleName="O2Store" level="5000">
  			<expFailureDecider lambda="0.00001447" ></expFailureDecider></O2Store>
  		<OGS moduleName="OGS1">
  			<expFailureDecider lambda="0.00046296" />
  			<powerConsumer inputs="PowerStore" desiredFlowRates="1000"
  				maxFlowRates="1500" />
  			<potableWaterConsumer inputs="PotableWaterStore" desiredFlowRates="10"
  				maxFlowRates="15" ></potableWaterConsumer>
  			<O2Producer desiredFlowRates="1000" outputs="O2Store"
  				maxFlowRates="1500" ></O2Producer>
  			<H2Producer desiredFlowRates="1000" outputs="H2Store"
  				maxFlowRates="1500" />
  		</OGS>
  		<OGS moduleName="OGS2">
  			<expFailureDecider lambda="0.00046296" ></expFailureDecider>
  			<powerConsumer inputs="PowerStore" desiredFlowRates="1000"
  				maxFlowRates="1500" ></powerConsumer>
  			<potableWaterConsumer inputs="PotableWaterStore" desiredFlowRates="10"
  				maxFlowRates="15" />
  			<O2Producer desiredFlowRates="1000" outputs="O2Store"
  				maxFlowRates="1500" />
  			<H2Producer desiredFlowRates="1000" outputs="H2Store"
  				maxFlowRates="1500" />
  		</OGS>
  		<VCCR moduleName="VCCR1">
  			<normalFailureDecider logmean="4320" logsd="3" />
  			<powerConsumer inputs="PowerStore" desiredFlowRates="2000"
  				maxFlowRates="2500" />
  			<airConsumer inputs="SimEnvironment" desiredFlowRates="10000"
  				maxFlowRates="15000" />
  			<airProducer desiredFlowRates="10000" outputs="SimEnvironment"
  				maxFlowRates="15000" />
  			<CO2Producer desiredFlowRates="10000" outputs="CO2Store"
  				maxFlowRates="15000" />
  		</VCCR>
  		<VCCR moduleName="VCCR2">
  			<normalFailureDecider logmean="4320" logsd="3" />
  			<powerConsumer inputs="PowerStore" desiredFlowRates="2000"
  				maxFlowRates="2500" ></powerConsumer>
  			<airConsumer inputs="SimEnvironment" desiredFlowRates="10000"
  				maxFlowRates="15000" ></airConsumer>
  			<airProducer desiredFlowRates="10000" outputs="SimEnvironment"
  				maxFlowRates="15000" />
  			<CO2Producer desiredFlowRates="10000" outputs="CO2Store"
  				maxFlowRates="15000" />
  		</VCCR></air>
  	<crew>
  		<CrewGroup moduleName="CrewGroup">
  			<normalFailureDecider logmean="4325.6" logsd="2" />
  			<potableWaterConsumer inputs="PotableWaterStore" desiredFlowRates="5"
  				maxFlowRates="10" />
  			<airConsumer inputs="SimEnvironment" desiredFlowRates="0"
  				maxFlowRates="2" />
  			<foodConsumer inputs="FoodStore" desiredFlowRates="5"
  				maxFlowRates="10" />
  			<dirtyWaterProducer desiredFlowRates="100" outputs="DirtyWaterStore"
  				maxFlowRates="150" ></dirtyWaterProducer>
  			<greyWaterProducer desiredFlowRates="100" outputs="GreyWaterStore"
  				maxFlowRates="150" />
  			<airProducer desiredFlowRates="0" outputs="SimEnvironment"
  				maxFlowRates="0" />
  			<dryWasteProducer desiredFlowRates="10" outputs="DryWasteStore"
  				maxFlowRates="15" />
  			<crewPerson age="22" name="Francisco" sex="MALE" weight="75">
  				<schedule>
						<activity intensity="2" name="ruminating" length="12" />
						<activity intensity="0" name="sleep" length="8" />
						<activity intensity="5" name="excercise" length="4" />
				</schedule>
			</crewPerson>
  		</CrewGroup></crew>
  	<environment>
  		<SimEnvironment moduleName="SimEnvironment" initialVolume="2700000"></SimEnvironment></environment>
  	<food>
  		<FoodStore capacity="100000" level="100000" moduleName="FoodStore">
  			<expFailureDecider lambda="0.00023148" /></FoodStore></food>
  	<framework>
  		<Injector moduleName="Injector1">
  			<normalFailureDecider logmean="3888" logsd="3" /><O2Consumer
  				inputs="O2Store" desiredFlowRates="5" maxFlowRates="10" />
  			<O2Producer
  				desiredFlowRates="5" outputs="SimEnvironment" maxFlowRates="10" ></O2Producer></Injector>
  		<Injector moduleName="Injector2">
  			<normalFailureDecider logmean="3888" logsd="3" /><O2Consumer
  				inputs="O2Store" desiredFlowRates="5" maxFlowRates="10" /><O2Producer
  				desiredFlowRates="5" outputs="SimEnvironment" maxFlowRates="10" ></O2Producer></Injector></framework>
  	<power>
  		<PowerStore capacity="100000" moduleName="PowerStore" level="100000"
  			logLevel="INFO">
  			<expFailureDecider lambda="0.00023148" />
  		</PowerStore></power>
  	<waste>
  		<DryWasteStore capacity="100000" moduleName="DryWasteStore" level="0.0">
  			<expFailureDecider lambda="0.00001447" ></expFailureDecider></DryWasteStore></waste>
  	<water>
  		<DirtyWaterStore capacity="1000" moduleName="DirtyWaterStore" level="0.0">
  			<expFailureDecider lambda="0.00001447" /></DirtyWaterStore>
  		<GreyWaterStore capacity="1000" moduleName="GreyWaterStore" level="0.0">
  			<expFailureDecider lambda="0.00001447" /></GreyWaterStore>
  		<PotableWaterStore capacity="2000" moduleName="PotableWaterStore"
  			level="1000">
  			<expFailureDecider lambda="0.00001447" />
  		</PotableWaterStore>
  		<WaterRS moduleName="WaterRS1">
  			<weibull2FailureDecider lambda="0.000231" beta="5" />
  			<powerConsumer inputs="PowerStore" desiredFlowRates="1000"
  				maxFlowRates="1500" />
  			<dirtyWaterConsumer inputs="DirtyWaterStore" desiredFlowRates="10"
  				maxFlowRates="15" ></dirtyWaterConsumer>
  			<greyWaterConsumer inputs="GreyWaterStore" desiredFlowRates="10"
  				maxFlowRates="15" ></greyWaterConsumer>
  			<potableWaterProducer desiredFlowRates="10" outputs="PotableWaterStore"
  				maxFlowRates="15" />
  		</WaterRS>
  		<WaterRS moduleName="WaterRS2">
  			<weibull2FailureDecider lambda="0.000231" beta="5" />
  			<powerConsumer inputs="PowerStore" desiredFlowRates="1000"
  				maxFlowRates="1500" ></powerConsumer>
  			<dirtyWaterConsumer inputs="DirtyWaterStore" desiredFlowRates="10"
  				maxFlowRates="15" />
  			<greyWaterConsumer inputs="GreyWaterStore" desiredFlowRates="10"
  				maxFlowRates="15" />
  			<potableWaterProducer desiredFlowRates="10" outputs="PotableWaterStore"
  				maxFlowRates="15" />
  		</WaterRS></water></SimBioModules>
  <Sensors>
  	<environment>
  		<GasPressureSensor input="SimEnvironment" moduleName="O2PressureSensor" gasType="O2"></GasPressureSensor>
  		<GasPressureSensor input="SimEnvironment" moduleName="CO2PressureSensor" gasType="CO2"></GasPressureSensor>
  		<GasPressureSensor input="SimEnvironment" moduleName="N2PressureSensor" gasType="NITROGEN"></GasPressureSensor>
  		<GasPressureSensor input="SimEnvironment" moduleName="VaporPressureSensor" gasType="VAPOR"></GasPressureSensor>
  		<AirInFlowRateSensor input="SimEnvironment" moduleName="AirInFlowRateSensor" index="0"></AirInFlowRateSensor>
  		<AirOutFlowRateSensor input="SimEnvironment" moduleName="AirOutFlowRateSensor" index="0"></AirOutFlowRateSensor></environment>
  	<air>
  		<CO2OutFlowRateSensor input="VCCR1"
  			moduleName="CO2OutFlowRateSensor" index="0"
  			isBionetEnabled="true">
  		</CO2OutFlowRateSensor></air></Sensors>
</biosim>
