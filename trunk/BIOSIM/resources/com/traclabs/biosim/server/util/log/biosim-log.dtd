<!DOCTYPE biosim [

<!ELEMENT biosim (tick*)>
    <!ELEMENT tick (SimEnvironment | PowerPS | WaterRS | CrewGroup | AirRS | FoodProcessor | BiomassRS | SimEnvironment)>
    <!ATTLIST tick number (CDATA) #REQUIRED>
    <!ELEMENT SimEnvironment (O2 Level, CO2 Level, other Level, light intensity)>
    <!ELEMENT PowerPS (#PCDATA)>
    <!ELEMENT WaterRS (#PCDATA)>
    <!ELEMENT CrewGroup (#PCDATA)>
    <!ELEMENT AirRS (#PCDATA)>
    <!ELEMENT FoodProcessor (#PCDATA)>
    <!ELEMENT BiomassRS (#PCDATA)>
    <!ELEMENT SimEnvironment (#PCDATA)>
]>
