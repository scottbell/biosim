<!DOCTYPE biosim [

<!ELEMENT biosim (tick*)>
    <!ELEMENT tick (SimEnvironment | PowerPS | WaterRS | CrewGroup | AirRS | FoodProcessor | BiomassRS | SimEnvironment)>
    <!ATTLIST tick number (CDATA) #REQUIRED>
    <!ELEMENT SimEnvironment (O2_level, CO2_level, other_level, capacity, light_intensity)>
    	<!ELEMENT O2_level (CDATA) #REQUIRED>
	<!ELEMENT CO2_level (CDATA) #REQUIRED>
	<!ELEMENT other_level (CDATA) #REQUIRED>
	<!ELEMENT capacity (CDATA) #REQUIRED>
	<!ELEMENT light_intensity (CDATA) #REQUIRED>
    <!ELEMENT PowerPS (power_produced)>
    	<!ELEMENT power_produced (CDATA) #REQUIRED>
    <!ELEMENT WaterRS (potable_water_produced, grey_water_produced, power_consumed, dirty_water_consumed, grey_water_consumed, AES, BWP, RO, PPS)>
    	<!ELEMENT potable_water_produced (CDATA) #REQUIRED>
	<!ELEMENT grey_water_produced (CDATA) #REQUIRED>
	<!ELEMENT power_consumed (CDATA) #REQUIRED>
	<!ELEMENT dirty_water_consumed (CDATA) #REQUIRED>
	<!ELEMENT grey_water_consumed (CDATA) #REQUIRED>
	<!ELEMENT AES (enabled, power_needed, power_consumed, water_needed, has_enough_power, has_enough_water, water_level)>
	<!ELEMENT BWP (enabled, power_needed, power_consumed, water_needed, has_enough_power, has_enough_water, water_level)>
	<!ELEMENT RO (enabled, power_needed, power_consumed, water_needed, has_enough_power, has_enough_water, water_level)>
	<!ELEMENT PPS (enabled, power_needed, power_consumed, water_needed, has_enough_power, has_enough_water, water_level)>	
		<!ELEMENT enabled (CDATA) #REQUIRED>
		<!ELEMENT power_needed (CDATA) #REQUIRED>
		<!ELEMENT power_consumed (CDATA) #REQUIRED>
		<!ELEMENT water_needed (CDATA) #REQUIRED>
		<!ELEMENT has_enough_power (CDATA) #REQUIRED>
		<!ELEMENT has_enough_water (CDATA) #REQUIRED>
		<!ELEMENT water_level (CDATA) #REQUIRED>
    <!ELEMENT CrewGroup (crew_person*)>
    	<!ELEMENT crew_person (name, current_activity, current_activity_order, duration_of_activity, starving_time, thirst_time, suffocate_time, poison_time, person_starving, person_thirsty, person_suffocating, person_poisoned, has_died, age, weight, sex, O2_consumed, CO2_produced, food_consumed, potable_water_consumed, dirty_water_produced, grey_water_produced, O2_needed, potable_water_needed, food_needed, air_retrieved)>
