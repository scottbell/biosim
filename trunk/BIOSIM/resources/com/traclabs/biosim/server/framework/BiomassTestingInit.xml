<?xml version="1.0"?>
<biosim xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:noNamespaceSchemaLocation='BiosimInitialization.xsd'>
	<Globals runTillDead="false" runTillN="2040" isFullLoggingEnabled="false" startPaused="false" logHandler="STANDARD_OUT">
		<comment>The default initialization for BioSim</comment>
		<author>Scott Bell</author>
	</Globals>
	<SimBioModules>
		<environment>
			<SimEnvironment name="PlantEnvironment" initialVolume="100000000000000"/>
			<Dehumidifier name="Dehumidifier">
				<airConsumer maxFlowRates="20000" desiredFlowRates="20000" inputs="PlantEnvironment"/>
				<dirtyWaterProducer maxFlowRates="20000" desiredFlowRates="20000" outputs="DirtyWaterStore"/>	
			</Dehumidifier>
		</environment>                    
		<food>
			<BiomassRS name="BiomassRS" autoHarvestAndReplant="false">
				<shelf cropArea="1" cropType="LETTUCE"/>
				<powerConsumer maxFlowRates="400" desiredFlowRates="400" inputs="PowerStore"/>
				<potableWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="PotableWaterStore"/>
				<greyWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="GreyWaterStore"/>
				<airConsumer maxFlowRates="0" desiredFlowRates="0" inputs="PlantEnvironment"/>
				<dirtyWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="DirtyWaterStore"/>
				<biomassProducer maxFlowRates="100" desiredFlowRates="100" outputs="BiomassStore"/>
				<airProducer maxFlowRates="0" desiredFlowRates="0" outputs="PlantEnvironment"/>
			</BiomassRS>
			<BiomassStore name="BiomassStore" capacity="500" level="300"/>
		</food>
		<power>
			<PowerStore name="PowerStore" capacity="100000000000" level="100000000000"/>
		</power>
		<water>
			<PotableWaterStore name="PotableWaterStore" capacity="500000000000" level="500000000000"/>
			<GreyWaterStore name="GreyWaterStore" capacity="500000000000" level="500000000000"/>
			<DirtyWaterStore name="DirtyWaterStore" capacity="500000000000" level="0"/>
		</water>
	</SimBioModules>
	<Sensors>
	</Sensors>
	<Actuators>
	</Actuators>
</biosim>
