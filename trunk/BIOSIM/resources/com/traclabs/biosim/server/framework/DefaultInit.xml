<?xml version="1.0"?>
<biosim xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:noNamespaceSchemaLocation='BiosimInitSchema.xsd'>
	<Globals crewsToWatch="CrewGroup" runTillN="3000">
		<comment>
			A default initialization of Biosim.
		</comment>
		<author>Scott Bell</author>
		<!-- <log4jProperty moduleName="log4j.logger.com.traclabs.biosim.server.sensor" value="DEBUG"/> !-->
	</Globals>
	<SimBioModules>
		<air>
			<VCCR moduleName="VCCR">
				<powerConsumer maxFlowRates="100000" desiredFlowRates="100000" inputs="PowerStore"/>
				<airConsumer maxFlowRates="10000000" desiredFlowRates="10000000" inputs="CrewEnvironment"/>
				<airProducer maxFlowRates="10000000" desiredFlowRates="10000000" outputs="CrewEnvironment"/>
				<CO2Producer maxFlowRates="10000000" desiredFlowRates="10000000" outputs="CO2Store"/>
			</VCCR>
			<OGS moduleName="OGS">
				<powerConsumer maxFlowRates="2500" desiredFlowRates="2500" inputs="PowerStore"/>
				<potableWaterConsumer maxFlowRates="10000000" desiredFlowRates="10000000" inputs="CrewPotableWaterStore"/>
				<O2Producer maxFlowRates="10000000" desiredFlowRates="10000000" outputs="O2Store"/>
				<H2Producer maxFlowRates="10000000" desiredFlowRates="10000000" outputs="H2Store"/>
			</OGS>
			<O2Store moduleName="O2Store" capacity="2000" level="2000"/>
			<CO2Store moduleName="CO2Store" capacity="0" level="0"/>
			<H2Store moduleName="H2Store" capacity="1000" level="1000"/>
			<NitrogenStore moduleName="NitrogenStore" capacity="5000" level="4000"/>
		</air>
		<crew>
			<CrewGroup moduleName="CrewGroup">
				<potableWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="CrewPotableWaterStore"/>
				<airConsumer maxFlowRates="0" desiredFlowRates="0" inputs="CrewEnvironment"/>
				<foodConsumer maxFlowRates="100" desiredFlowRates="100" inputs="FoodStore"/>
				<dirtyWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="CrewDirtyWaterStore"/>
				<greyWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="CrewGreyWaterStore"/>
				<airProducer maxFlowRates="0" desiredFlowRates="0" outputs="CrewEnvironment"/>
				<dryWasteProducer maxFlowRates="100" desiredFlowRates="100" outputs="DryWasteStore"/>
				<crewPerson name="Bob Roberts" age="35" weight="70" sex="MALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
				<crewPerson name="Stephanie Stevens" age="32" weight="71" sex="FEMALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
				<crewPerson name="Bill Williams" age="30" weight="72" sex="MALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
				<crewPerson name="Janet Janey" age="34" weight="75" sex="FEMALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
			</CrewGroup>
		</crew>
		<environment>
			<SimEnvironment moduleName="CrewEnvironment" initialVolume="20000000" leakRate="0.00224" airlockVolume="3.7"/>
			<SimEnvironment moduleName="PlantEnvironment" initialVolume="20000000" leakRate="0.00224" airlockVolume="3.7"/>
			<Dehumidifier moduleName="PlantDehumidifier">
				<airConsumer maxFlowRates="200000" desiredFlowRates="200000" inputs="PlantEnvironment"/>
				<dirtyWaterProducer maxFlowRates="200000" desiredFlowRates="200000" outputs="PlantDirtyWaterStore"/>	
			</Dehumidifier>
			<Dehumidifier moduleName="CrewDehumidifier">
				<airConsumer maxFlowRates="200000" desiredFlowRates="200000" inputs="CrewEnvironment"/>
				<dirtyWaterProducer maxFlowRates="200000" desiredFlowRates="200000" outputs="CrewDirtyWaterStore"/>	
			</Dehumidifier>
		</environment>                    
		<food>
			<FoodProcessor moduleName="FoodProcessor">
				<powerConsumer maxFlowRates="100" desiredFlowRates="100" inputs="PowerStore"/>
				<biomassConsumer maxFlowRates="200" desiredFlowRates="200" inputs="BiomassStore"/>
				<foodProducer maxFlowRates="200" desiredFlowRates="200" outputs="FoodStore"/>
				<dryWasteProducer maxFlowRates="100" desiredFlowRates="100" outputs="DryWasteStore"/>
				<waterProducer maxFlowRates="100 100" desiredFlowRates="100 100" outputs="PlantDirtyWaterStore PlantGreyWaterStore"/>
			</FoodProcessor>
			<BiomassPS moduleName="BiomassPS">
				<shelf cropArea="20" cropType="LETTUCE"/>
				<shelf cropArea="20" cropType="TOMATO"/>
				<powerConsumer maxFlowRates="400" desiredFlowRates="400" inputs="PowerStore"/>
				<potableWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="PlantPotableWaterStore"/>
				<greyWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="PlantGreyWaterStore"/>
				<airConsumer maxFlowRates="0" desiredFlowRates="0" inputs="PlantEnvironment"/>
				<dirtyWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="PlantDirtyWaterStore"/>
				<biomassProducer maxFlowRates="100" desiredFlowRates="100" outputs="BiomassStore"/>
				<airProducer maxFlowRates="0" desiredFlowRates="0" outputs="PlantEnvironment"/>
			</BiomassPS>
			<BiomassStore moduleName="BiomassStore" capacity="1000" level="300"/>
			<FoodStore moduleName="FoodStore" capacity="2000" level="2000"/>
		</food>
		<framework>
			<Accumulator moduleName="PlantAccumulator">
				<O2Consumer desiredFlowRates="100000000" maxFlowRates="100000000" inputs="PlantEnvironment"/>
				<O2Producer desiredFlowRates="100000000" maxFlowRates="100000000" outputs="O2Store"/>
			</Accumulator>
			<Injector moduleName="Injector">
				<O2Consumer desiredFlowRates="30" maxFlowRates="30" inputs="O2Store"/>
				<O2Producer  desiredFlowRates="30" maxFlowRates="30" outputs="CrewEnvironment"/>
			</Injector>
		</framework>
		<power>
			<PowerPS moduleName="PowerPS" generationType="SOLAR" upperPowerGeneration="70000000">
				<lightConsumer maxFlowRates="0" desiredFlowRates="0" inputs="CrewEnvironment"/>
				<powerProducer maxFlowRates="5000000" desiredFlowRates="5000000" outputs="PowerStore"/>
			</PowerPS>
			<PowerStore moduleName="PowerStore" capacity="5000000" level="5000000"/>
		</power>
		<water>
			<WaterRS moduleName="CrewWaterRS" implementation="LINEAR">
				<powerConsumer maxFlowRates="3000" desiredFlowRates="3000" inputs="PowerStore"/>
				<dirtyWaterConsumer maxFlowRates="10000000" desiredFlowRates="10000000" inputs="CrewDirtyWaterStore"/>
				<greyWaterConsumer maxFlowRates="10000000" desiredFlowRates="10000000" inputs="CrewGreyWaterStore"/>
				<potableWaterProducer maxFlowRates="10000000" desiredFlowRates="10000000" outputs="CrewPotableWaterStore"/>
			</WaterRS>
				<WaterRS moduleName="PlantWaterRS" implementation="LINEAR">
				<powerConsumer maxFlowRates="3000" desiredFlowRates="3000" inputs="PowerStore"/>
				<dirtyWaterConsumer maxFlowRates="10000000" desiredFlowRates="10000000" inputs="PlantDirtyWaterStore"/>
				<greyWaterConsumer maxFlowRates="10000000" desiredFlowRates="10000000" inputs="PlantGreyWaterStore"/>
				<potableWaterProducer maxFlowRates="10000000" desiredFlowRates="10000000" outputs="PlantPotableWaterStore"/>
			</WaterRS>
			<PotableWaterStore moduleName="CrewPotableWaterStore" capacity="800000" level="500000"/>
			<PotableWaterStore moduleName="PlantPotableWaterStore" capacity="800000" level="500000"/>
			<GreyWaterStore moduleName="CrewGreyWaterStore" capacity="4000" level="0"/>
			<GreyWaterStore moduleName="PlantGreyWaterStore" capacity="4000" level="0"/>
			<DirtyWaterStore moduleName="CrewDirtyWaterStore" capacity="5000" level="0"/>
			<DirtyWaterStore moduleName="PlantDirtyWaterStore" capacity="5000" level="0"/>
		</water>
		<waste>
			<DryWasteStore moduleName="DryWasteStore" capacity="500" level="0"/>
		</waste>
	</SimBioModules>
	<Sensors>
		<environment>
			<GasConcentrationSensor gasType="CO2" input="CrewEnvironment" moduleName="CrewEnvironmentCO2ConcentrationSensor"/>
			<GasConcentrationSensor gasType="O2" input="CrewEnvironment" moduleName="CrewEnvironmentO2ConcentrationSensor"/>
			<GasConcentrationSensor gasType="VAPOR" input="CrewEnvironment" moduleName="CrewEnvironmentVaporConcentrationSensor"/>
			<GasPressureSensor gasType="VAPOR" input="CrewEnvironment" moduleName="CrewEnvironmentVaporPressureSensor"/>
		</environment>
		<food>
			<BiomassInFlowRateSensor index="0" input="FoodProcessor" moduleName="FoodProcessorBiomassInFlowRateSensor"/>
		</food>
		<framework>
			<StoreLevelSensor input="CO2Store" moduleName="CO2StoreLevelSensor"/>
			<StoreLevelSensor input="O2Store" moduleName="O2StoreLevelSensor"/>
			<StoreLevelSensor input="H2Store" moduleName="H2StoreLevelSensor"/>
			<StoreLevelSensor input="BiomassStore" moduleName="BiomassStoreLevelSensor"/>
			<StoreLevelSensor input="FoodStore" moduleName="FoodStoreLevelSensor"/>
			<StoreLevelSensor input="PowerStore" moduleName="PowerStoreLevelSensor"/>
			<StoreLevelSensor input="CrewDirtyWaterStore" moduleName="CrewDirtyWaterStoreLevelSensor"/>
			<StoreLevelSensor input="CrewGreyWaterStore" moduleName="CrewGreyWaterStoreLevelSensor"/>
			<StoreLevelSensor input="CrewPotableWaterStore" moduleName="CrewPotableWaterStoreLevelSensor"/>
			<StoreOverflowSensor input="O2Store" moduleName="O2StoreOverflowSensor"/>
			<StoreOverflowSensor input="CO2Store" moduleName="CO2StoreOverflowSensor"/>
		</framework>
		<power>
			<PowerInFlowRateSensor index="0" input="FoodProcessor" moduleName="FoodProcessorPowerInFlowRateSensor"/>
		</power>
	</Sensors>
	<Actuators>
		<air>
			<O2OutFlowRateActuator index="0" output="OGS" moduleName="OGSO2OutFlowRateActuator"/>
			<H2OutFlowRateActuator index="0" output="OGS" moduleName="OGSH2OutFlowRateActuator"/>
			<O2OutFlowRateActuator index="0" output="Injector" moduleName="InjectorO2OutFlowRateActuator"/>
			<O2InFlowRateActuator index="0" output="Injector" moduleName="InjectorO2InFlowRateActuator"/>
		</air>
		<food>
			<BiomassInFlowRateActuator index="0" output="FoodProcessor" moduleName="FoodProcessorBiomassInFlowRateActuator"/>
			<HarvestingActuator output="BiomassPS" moduleName="BiomassPSHarvestingActuator" shelfIndex="0"/>
			<PlantingActuator output="BiomassPS" moduleName="BiomassPSPlantingActuator" shelfIndex="0"/>
		</food>
		<power>
			<PowerInFlowRateActuator index="0" output="FoodProcessor" moduleName="FoodProcessorPowerInFlowRateActuator"/>
			<PowerInFlowRateActuator index="0" output="OGS" moduleName="OGSPowerInFlowRateActuator"/>
			<PowerInFlowRateActuator index="0" output="CrewWaterRS" moduleName="CrewWaterRSPowerInFlowRateActuator"/>
		</power>
		<water>
			<DirtyWaterInFlowRateActuator index="0" output="CrewWaterRS" moduleName="CrewWaterRSDirtyWaterInFlowRateActuator"/>
			<GreyWaterInFlowRateActuator index="0" output="BiomassPS" moduleName="BiomassPSGreyWaterInFlowRateActuator"/>
			<GreyWaterInFlowRateActuator index="0" output="CrewWaterRS" moduleName="CrewWaterRSGreyWaterInFlowRateActuator"/>
			<PotableWaterInFlowRateActuator index="0" output="OGS" moduleName="OGSPotableWaterInFlowRateActuator"/>
			<PotableWaterOutFlowRateActuator index="0" output="CrewWaterRS" moduleName="OGSPotableWaterOutFlowRateActuator"/>
		</water>
	</Actuators>
</biosim>

