	<xsd:complexType name="SimEnvironmentType">  
		<xsd:complexContent>
			<xsd:extension base="BioModuleType">
				<xsd:attribute name="initialVolume" type="nonNegativeFloat" use="required"/>
				<xsd:attribute name="initialCO2Moles" type="nonNegativeFloat" use="optional"/>
				<xsd:attribute name="initialO2Moles" type="nonNegativeFloat" use="optional"/>
				<xsd:attribute name="initialOtherMoles" type="nonNegativeFloat" use="optional"/>
				<xsd:attribute name="initialWaterMoles" type="nonNegativeFloat" use="optional"/>
				<xsd:attribute name="initialNitrogenMoles" type="nonNegativeFloat" use="optional"/>
				<xsd:attribute name="leakRate" type="nonNegativeFloat" default="0"/>
				<xsd:attribute name="dayLength" type="nonNegativeFloat" default="24"/>
				<xsd:attribute name="hourOfDayStart" type="nonNegativeFloat" default="0"/>
				<xsd:attribute name="maxLumens" type="nonNegativeFloat" default="50000"/>
				<xsd:attribute name="airlockVolume" type="nonNegativeFloat" default="3.7"/>
				<xsd:attribute name="dangerousOxygenThreshold" type="percentageFloat" default="1.0"/>
			</xsd:extension>
		</xsd:complexContent>
