<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:annotation>
		<xsd:documentation xml:lang="en">BioSim Initialization Schema</xsd:documentation>
	</xsd:annotation>
	
	<!-- Simple types -->
	<xsd:simpleType name="floatList">
		<xsd:list itemType="nonNegativeFloat"/>
	</xsd:simpleType>
	
	<xsd:simpleType name="stringList">
		<xsd:list itemType="xsd:string"/>
	</xsd:simpleType>
	
	<xsd:simpleType name="nonNegativeFloat">
		<xsd:restriction base="xsd:float">
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="percentageFloat">
		<xsd:restriction base="xsd:float">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<!-- Top level element declaration -->	
	<xsd:element name="biosim" type="BioSimType"/>
	
	<xsd:complexType name="BioSimType">
		<xsd:sequence>
			<xsd:element name="Globals" type="GlobalsType"/>
			<xsd:element name="SimBioModules" type="SimBioModulesType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Sensors" type="SensorsType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Actuators" type="ActuatorsType" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>     
	</xsd:complexType>
	
	<xsd:complexType name="GlobalsType">
		<xsd:sequence>
			<xsd:element name="comment" type="xsd:string" minOccurs="0" maxOccurs="1"/>	
			<xsd:element name="author" type="xsd:string" minOccurs="0" maxOccurs="1"/>	
			<xsd:element name="log4jProperty" type="log4jPropertyType" minOccurs="0" maxOccurs="unbounded"/>	
		</xsd:sequence>
		<xsd:attribute name="startPaused" type="xsd:boolean" default="true"/>
		<xsd:attribute name="runTillN" type="xsd:nonNegativeInteger" default="0"/>
		<xsd:attribute name="runTillCrewDeath" type="xsd:boolean" default="true"/>
		<xsd:attribute name="runTillPlantDeath" type="xsd:boolean" default="false"/>
		<xsd:attribute name="crewsToWatch" type="stringList" use="optional"/>
		<xsd:attribute name="plantsToWatch" type="stringList" use="optional"/>
		<xsd:attribute name="driverStutterLength" type="xsd:integer" default="-1"/>     
		<xsd:attribute name="isLooping" type="xsd:boolean" default="false"/>
		<xsd:attribute name="stochasticIntensity" type="StochasticIntensityType" default="NONE_STOCH"/>
	</xsd:complexType>
	
	<xsd:complexType name="log4jPropertyType">
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	
	<xsd:simpleType name="StochasticIntensityType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="HIGH_STOCH"/>
			<xsd:enumeration value="MEDIUM_STOCH"/>
			<xsd:enumeration value="LOW_STOCH"/>
			<xsd:enumeration value="NONE_STOCH"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="SimBioModulesType">
		<xsd:all>
			<xsd:element name="air" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="AirRS" type="AirRSType"/>
							<xsd:element name="OGS" type="OGSType" />
							<xsd:element name="CRS" type="CRSType"/>
							<xsd:element name="VCCR" type="VCCRType"/>
							<xsd:element name="O2Store" type="O2StoreType"/>
							<xsd:element name="CO2Store" type="CO2StoreType"/>
							<xsd:element name="H2Store" type="H2StoreType"/>
							<xsd:element name="NitrogenStore" type="NitrogenStoreType"/>
							<xsd:element name="MethaneStore" type="NitrogenStoreType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="crew" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="CrewGroup" type="CrewGroupType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="environment" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="SimEnvironment" type="SimEnvironmentType"/>
							<xsd:element name="Dehumidifier" type="DehumidifierType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="food" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="FoodProcessor" type="FoodProcessorType"/>
							<xsd:element name="BiomassRS" type="BiomassRSType"/>
							<xsd:element name="BiomassStore" type="BiomassStoreType"/>
							<xsd:element name="FoodStore" type="FoodStoreType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="power" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>	
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="PowerPS" type="PowerPSType"/>
							<xsd:element name="PowerStore" type="PowerStoreType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="water" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="WaterRS" type="WaterRSType"/>
							<xsd:element name="PotableWaterStore" type="PotableWaterStoreType"/>
							<xsd:element name="GreyWaterStore" type="GreyWaterStoreType"/>
							<xsd:element name="DirtyWaterStore" type="DirtyWaterStoreType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="waste" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="Incinerator" type="IncineratorType"/>
							<xsd:element name="DryWasteStore" type="DryWasteStoreType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="framework" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="Accumulator" type="AccumulatorType"/>
							<xsd:element name="Injector" type="InjectorType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="mission" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="EVAMission" type="EVAMissionType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>     
	</xsd:complexType>
	
	<xsd:complexType name="SensorsType">
		<xsd:all>
			<xsd:element name="air" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="CO2InFlowRateSensor" type="CO2InFlowRateSensorType"/>
							<xsd:element name="CO2OutFlowRateSensor" type="CO2OutFlowRateSensorType"/>
							<xsd:element name="CO2StoreLevelSensor" type="CO2StoreLevelSensorType"/>
							<xsd:element name="O2InFlowRateSensor" type="O2InFlowRateSensorType"/>
							<xsd:element name="O2OutFlowRateSensor" type="O2OutFlowRateSensorType"/>
							<xsd:element name="O2StoreLevelSensor" type="O2StoreLevelSensorType"/>
							<xsd:element name="H2InFlowRateSensor" type="H2InFlowRateSensorType"/>
							<xsd:element name="H2OutFlowRateSensor" type="H2OutFlowRateSensorType"/>
							<xsd:element name="H2StoreLevelSensor" type="H2StoreLevelSensorType"/>
							<xsd:element name="NitrogenInFlowRateSensor" type="NitrogenInFlowRateSensorType"/>
							<xsd:element name="NitrogenOutFlowRateSensor" type="NitrogenOutFlowRateSensorType"/>
							<xsd:element name="NitrogenStoreLevelSensor" type="NitrogenStoreLevelSensorType"/>
							<xsd:element name="MethaneOutFlowRateSensor" type="MethaneOutFlowRateSensorType"/>
							<xsd:element name="MethaneStoreLevelSensor" type="MethaneStoreLevelSensorType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="crew" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="CrewGroupDeathSensor" type="CrewGroupDeathSensorType"/>			
							<xsd:element name="CrewGroupProductivitySensor" type="CrewGroupProductivitySensorType"/>			
							<xsd:element name="CrewGroupAnyDeadSensor" type="CrewGroupAnyDeadSensorType"/>
						</xsd:choice>			
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="environment" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="AirInFlowRateSensor" type="AirInFlowRateSensorType"/>
							<xsd:element name="AirOutFlowRateSensor" type="AirOutFlowRateSensorType"/>
							<xsd:element name="CO2AirConcentrationSensor" type="CO2AirConcentrationSensorType"/>
							<xsd:element name="CO2AirEnvironmentInFlowRateSensor" type="CO2AirEnvironmentInFlowRateSensorType"/>
							<xsd:element name="CO2AirEnvironmentOutFlowRateSensor" type="CO2AirEnvironmentOutFlowRateSensorType"/>
							<xsd:element name="CO2AirPressureSensor" type="CO2AirPressureSensorType"/>
							<xsd:element name="CO2AirStoreInFlowRateSensor" type="CO2AirStoreInFlowRateSensorType"/>
							<xsd:element name="CO2AirStoreOutFlowRateSensor" type="CO2AirStoreOutFlowRateSensorType"/>
							<xsd:element name="O2AirConcentrationSensor" type="O2AirConcentrationSensorType"/>
							<xsd:element name="O2AirEnvironmentInFlowRateSensor" type="O2AirEnvironmentInFlowRateSensorType"/>
							<xsd:element name="O2AirEnvironmentOutFlowRateSensor" type="O2AirEnvironmentOutFlowRateSensorType"/>
							<xsd:element name="O2AirPressureSensor" type="O2AirPressureSensorType"/>
							<xsd:element name="O2AirStoreInFlowRateSensor" type="O2AirStoreInFlowRateSensorType"/>
							<xsd:element name="O2AirStoreOutFlowRateSensor" type="O2AirStoreOutFlowRateSensorType"/>
							<xsd:element name="NitrogenAirConcentrationSensor" type="NitrogenAirConcentrationSensorType"/>
							<xsd:element name="NitrogenAirPressureSensor" type="NitrogenAirPressureSensorType"/>
							<xsd:element name="NitrogenAirEnvironmentInFlowRateSensor" type="NitrogenAirEnvironmentInFlowRateSensorType"/>
							<xsd:element name="NitrogenAirEnvironmentOutFlowRateSensor" type="NitrogenAirEnvironmentOutFlowRateSensorType"/>
							<xsd:element name="NitrogenAirStoreInFlowRateSensor" type="NitrogenAirStoreInFlowRateSensorType"/>
							<xsd:element name="NitrogenAirStoreOutFlowRateSensor" type="NitrogenAirStoreOutFlowRateSensorType"/>
							<xsd:element name="WaterAirConcentrationSensor" type="WaterAirConcentrationSensorType"/>
							<xsd:element name="WaterAirPressureSensor" type="WaterAirPressureSensorType"/>
							<xsd:element name="WaterAirEnvironmentInFlowRateSensor" type="WaterAirEnvironmentInFlowRateSensorType"/>
							<xsd:element name="WaterAirEnvironmentOutFlowRateSensor" type="WaterAirEnvironmentOutFlowRateSensorType"/>
							<xsd:element name="WaterAirStoreInFlowRateSensor" type="WaterAirStoreInFlowRateSensorType"/>
							<xsd:element name="WaterAirStoreOutFlowRateSensor" type="WaterAirStoreOutFlowRateSensorType"/>	
							<xsd:element name="OtherAirConcentrationSensor" type="OtherAirConcentrationSensorType"/>
							<xsd:element name="OtherAirPressureSensor" type="OtherAirPressureSensorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="food" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="BiomassInFlowRateSensor" type="BiomassInFlowRateSensorType"/>
							<xsd:element name="BiomassOutFlowRateSensor" type="BiomassOutFlowRateSensorType"/>
							<xsd:element name="BiomassStoreLevelSensor" type="BiomassStoreLevelSensorType"/>
							<xsd:element name="FoodInFlowRateSensor" type="FoodInFlowRateSensorType"/>
							<xsd:element name="FoodOutFlowRateSensor" type="FoodOutFlowRateSensorType"/>
							<xsd:element name="FoodStoreLevelSensor" type="FoodStoreLevelSensorType"/>
							<xsd:element name="HarvestSensor" type="HarvestSensorType"/>
							<xsd:element name="PlantDeathSensor" type="PlantDeathSensorType"/>
							<xsd:element name="BiomassStoreWaterContentSensor" type="BiomassStoreWaterContentSensorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="framework" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="StoreLevelSensor" type="StoreLevelSensorType"/>
							<xsd:element name="StoreOverflowSensor" type="StoreLevelSensorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="power" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="PowerInFlowRateSensor" type="PowerInFlowRateSensorType"/>
							<xsd:element name="PowerOutFlowRateSensor" type="PowerOutFlowRateSensorType"/>
							<xsd:element name="PowerStoreLevelSensor" type="PowerStoreLevelSensorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="water" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="DirtyWaterInFlowRateSensor" type="DirtyWaterInFlowRateSensorType"/>
							<xsd:element name="DirtyWaterOutFlowRateSensor" type="DirtyWaterOutFlowRateSensorType"/>
							<xsd:element name="DirtyWaterStoreLevelSensor" type="DirtyWaterStoreLevelSensorType"/>
							<xsd:element name="GreyWaterInFlowRateSensor" type="GreyWaterInFlowRateSensorType"/>
							<xsd:element name="GreyWaterOutFlowRateSensor" type="GreyWaterOutFlowRateSensorType"/>
							<xsd:element name="GreyWaterStoreLevelSensor" type="GreyWaterStoreLevelSensorType"/>
							<xsd:element name="PotableWaterInFlowRateSensor" type="PotableWaterInFlowRateSensorType"/>
							<xsd:element name="PotableWaterOutFlowRateSensor" type="PotableWaterOutFlowRateSensorType"/>
							<xsd:element name="PotableWaterStoreLevelSensor" type="PotableWaterStoreLevelSensorType"/>
							<xsd:element name="WaterInFlowRateSensor" type="WaterInFlowRateSensorType"/>
							<xsd:element name="WaterOutFlowRateSensor" type="WaterOutFlowRateSensorType"/>
							<xsd:element name="WaterStoreLevelSensor" type="WaterStoreLevelSensorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="waste" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="DryWasteInFlowRateSensor" type="DryWasteInFlowRateSensorType"/>
							<xsd:element name="DryWasteOutFlowRateSensor" type="DryWasteOutFlowRateSensorType"/>
							<xsd:element name="DryWasteStoreLevelSensor" type="DryWasteStoreLevelSensorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>     
	</xsd:complexType>
	
	<xsd:complexType name="ActuatorsType">
		<xsd:all>
			<xsd:element name="air" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="CO2InFlowRateActuator" type="CO2InFlowRateActuatorType"/>
							<xsd:element name="CO2OutFlowRateActuator" type="CO2OutFlowRateActuatorType"/>
							<xsd:element name="O2InFlowRateActuator" type="O2InFlowRateActuatorType"/>
							<xsd:element name="O2OutFlowRateActuator" type="O2OutFlowRateActuatorType"/>
							<xsd:element name="H2InFlowRateActuator" type="H2InFlowRateActuatorType"/>
							<xsd:element name="H2OutFlowRateActuator" type="H2OutFlowRateActuatorType"/>
							<xsd:element name="NitrogenInFlowRateActuator" type="NitrogenInFlowRateActuatorType"/>
							<xsd:element name="NitrogenOutFlowRateActuator" type="NitrogenOutFlowRateActuatorType"/>
							<xsd:element name="MethaneInFlowRateActuator" type="MethaneInFlowRateActuatorType"/>
							<xsd:element name="MethaneOutFlowRateActuator" type="MethaneOutFlowRateActuatorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="crew" minOccurs="0" maxOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="environment" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="AirInFlowRateActuator" type="AirInFlowRateActuatorType"/>
							<xsd:element name="AirOutFlowRateActuator" type="AirOutFlowRateActuatorType"/>
							<xsd:element name="CO2AirEnvironmentInFlowRateActuator" type="CO2AirEnvironmentInFlowRateActuatorType"/>
							<xsd:element name="CO2AirEnvironmentOutFlowRateActuator" type="CO2AirEnvironmentOutFlowRateActuatorType"/>
							<xsd:element name="CO2AirStoreInFlowRateActuator" type="CO2AirStoreInFlowRateActuatorType"/>
							<xsd:element name="CO2AirStoreOutFlowRateActuator" type="CO2AirStoreOutFlowRateActuatorType"/>
							<xsd:element name="O2AirEnvironmentInFlowRateActuator" type="O2AirEnvironmentInFlowRateActuatorType"/>
							<xsd:element name="O2AirEnvironmentOutFlowRateActuator" type="O2AirEnvironmentOutFlowRateActuatorType"/>
							<xsd:element name="O2AirStoreInFlowRateActuator" type="O2AirStoreInFlowRateActuatorType"/>
							<xsd:element name="O2AirStoreOutFlowRateActuator" type="O2AirStoreOutFlowRateActuatorType"/>
							<xsd:element name="NitrogenAirEnvironmentInFlowRateActuator" type="NitrogenAirEnvironmentInFlowRateActuatorType"/>
							<xsd:element name="NitrogenAirEnvironmentOutFlowRateActuator" type="NitrogenAirEnvironmentOutFlowRateActuatorType"/>
							<xsd:element name="NitrogenAirStoreInFlowRateActuator" type="NitrogenAirStoreInFlowRateActuatorType"/>
							<xsd:element name="NitrogenAirStoreOutFlowRateActuator" type="NitrogenAirStoreOutFlowRateActuatorType"/>
							<xsd:element name="WaterAirEnvironmentInFlowRateActuator" type="WaterAirEnvironmentInFlowRateActuatorType"/>
							<xsd:element name="WaterAirEnvironmentOutFlowRateActuator" type="WaterAirEnvironmentOutFlowRateActuatorType"/>
							<xsd:element name="WaterAirStoreInFlowRateActuator" type="WaterAirStoreInFlowRateActuatorType"/>
							<xsd:element name="WaterAirStoreOutFlowRateActuator" type="WaterAirStoreOutFlowRateActuatorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="food" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="BiomassInFlowRateActuator" type="BiomassInFlowRateActuatorType"/>
							<xsd:element name="BiomassOutFlowRateActuator" type="BiomassOutFlowRateActuatorType"/>
							<xsd:element name="FoodInFlowRateActuator" type="FoodInFlowRateActuatorType"/>
							<xsd:element name="FoodOutFlowRateActuator" type="FoodOutFlowRateActuatorType"/>
							<xsd:element name="HarvestingActuator" type="HarvestingActuatorType"/>
							<xsd:element name="PlantingActuator" type="PlantingActuatorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="power" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="PowerInFlowRateActuator" type="PowerInFlowRateActuatorType"/>
							<xsd:element name="PowerOutFlowRateActuator" type="PowerOutFlowRateActuatorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="water" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="DirtyWaterInFlowRateActuator" type="DirtyWaterInFlowRateActuatorType"/>
							<xsd:element name="DirtyWaterOutFlowRateActuator" type="DirtyWaterOutFlowRateActuatorType"/>
							<xsd:element name="GreyWaterInFlowRateActuator" type="GreyWaterInFlowRateActuatorType"/>
							<xsd:element name="GreyWaterOutFlowRateActuator" type="GreyWaterOutFlowRateActuatorType"/>
							<xsd:element name="PotableWaterInFlowRateActuator" type="PotableWaterInFlowRateActuatorType"/>
							<xsd:element name="PotableWaterOutFlowRateActuator" type="PotableWaterOutFlowRateActuatorType"/>
							<xsd:element name="WaterInFlowRateActuator" type="WaterInFlowRateActuatorType"/>
							<xsd:element name="WaterOutFlowRateActuator" type="WaterOutFlowRateActuatorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="waste" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="DryWasteInFlowRateActuator" type="DryWasteInFlowRateActuatorType"/>
							<xsd:element name="DryWasteOutFlowRateActuator" type="DryWasteOutFlowRateActuatorType"/>
						</xsd:choice>				
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:all>     
	</xsd:complexType>
	
	<!-- Producer/Consumer types -->
	<xsd:complexType name="FlowRateType">  
		<xsd:attribute name="maxFlowRates" type="floatList" use="required"/>
		<xsd:attribute name="desiredFlowRates" type="floatList" use="required"/>
	</xsd:complexType>
	
	<xsd:complexType name="ConsumerType">  
		<xsd:complexContent>
			<xsd:extension base="FlowRateType">
					<xsd:attribute name="inputs" type="stringList" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="ProducerType">  
		<xsd:complexContent>
			<xsd:extension base="FlowRateType">
					<xsd:attribute name="outputs" type="stringList" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PowerConsumerType">  
		<xsd:complexContent>
			<xsd:extension base="ConsumerType">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PotableWaterConsumerType">  
		<xsd:complexContent>
			<xsd:extension base="ConsumerType">
