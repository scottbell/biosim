		<xsd:complexContent>
			<xsd:extension base="BioModuleType">
				<xsd:sequence>
					<xsd:choice minOccurs="11" maxOccurs="11">
						<xsd:element name="powerConsumer" type="PowerConsumerType"/>
						<xsd:element name="potableWaterConsumer" type="PotableWaterConsumerType"/>
						<xsd:element name="airConsumer" type="AirConsumerType"/>
						<xsd:element name="CO2Consumer" type="CO2ConsumerType"/>
						<xsd:element name="H2Consumer" type="H2ConsumerType"/>
						<xsd:element name="potableWaterProducer" type="PotableWaterProducerType"/>
						<xsd:element name="O2Producer" type="O2ProducerType"/>
						<xsd:element name="airProducer" type="AirProducerType"/>
						<xsd:element name="CO2Producer" type="CO2ProducerType"/>
						<xsd:element name="H2Producer" type="H2ProducerType"/>
						<xsd:element name="methaneProducer" type="MethaneProducerType"/>
					</xsd:choice>
				</xsd:sequence>
				<!--<xsd:attribute name="operationMode" type="AirRSOperationMode" default="FULL"/> -->
				<xsd:attribute name="implementation" type="AirRSImplementationType" default="NORMAL"/>
			</xsd:extension>
