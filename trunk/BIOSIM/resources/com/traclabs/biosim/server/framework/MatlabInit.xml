<?xml version="1.0"?>
<biosim xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:noNamespaceSchemaLocation='BiosimInitSchema.xsd'>
	<Globals>
		<comment>initialization for Matlab test</comment>
		<author>Scott Bell</author>
	</Globals>
	<SimBioModules>
		<power>
			<PowerStore moduleName="PowerStore" capacity="1000000" level="1000000"/>
		</power>
		<water>
			<WaterRS moduleName="WaterRS" implementation="MATLAB">
				<powerConsumer maxFlowRates="500" desiredFlowRates="500" inputs="PowerStore"/>
				<dirtyWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="DirtyWaterStore"/>
				<greyWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="GreyWaterStore"/>
				<potableWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="PotableWaterStore"/>
			</WaterRS>
			<PotableWaterStore moduleName="PotableWaterStore" capacity="50000" level="0"/>
			<GreyWaterStore moduleName="GreyWaterStore" capacity="50000" level="5000"/>
			<DirtyWaterStore moduleName="DirtyWaterStore" capacity="50000" level="2500"/>
		</water>
	</SimBioModules>
</biosim>
