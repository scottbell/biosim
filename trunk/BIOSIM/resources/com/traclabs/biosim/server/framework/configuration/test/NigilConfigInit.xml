<?xml version="1.0" encoding="UTF-8"?>
<p:biosim xmlns:p="http://www.traclabs.com/biosim" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.traclabs.com/biosim ../../schema/BiosimInitSchema.xsd ">
  <p:Globals crewsToWatch="Nigil" runTillCrewDeath="true"/>
  <p:SimBioModules>
  	<p:crew>
  		<p:CrewGroup moduleName="Nigil">
  			<p:potableWaterConsumer inputs="PotableWaterStore" desiredFlowRates="3"
  				maxFlowRates="3" />
  			<p:airConsumer inputs="SimEnvironment" desiredFlowRates="1000"
  				maxFlowRates="1000" />
  			<p:foodConsumer inputs="FoodStore" desiredFlowRates="100"
  				maxFlowRates="100" />
  			<p:dirtyWaterProducer desiredFlowRates="2" outputs="DirtyWaterStore"
  				maxFlowRates="2" />
  			<p:greyWaterProducer desiredFlowRates="100" outputs="GreyWaterStore"
  				maxFlowRates="100" />
  			<p:airProducer desiredFlowRates="100" outputs="SimEnvironment"
  				maxFlowRates="100" />
  			<p:dryWasteProducer desiredFlowRates="100" outputs="DryWasteStore"
  				maxFlowRates="100" /><p:crewPerson age="35" name="Nigil"
  				sex="MALE" weight="75">
  				<p:schedule>
  					<p:activity intensity="1" name="sleep" length="8" /><p:activity
  						intensity="2" name="hygiene" length="1" /><p:activity
  						intensity="5" name="exercise" length="1" /><p:activity
  						intensity="2" name="eating" length="1" /><p:activity
  						intensity="3" name="mission" length="9" /><p:activity
  						intensity="2" name="health" length="1" /><p:activity
  						intensity="2" name="maintenance" length="1" /><p:activity
  						intensity="2" name="leisure" length="2" />
  				</p:schedule>
</p:crewPerson>
  		</p:CrewGroup></p:crew>
  	<p:environment>
  		<p:SimEnvironment moduleName="SimEnvironment" initialVolume="32000"></p:SimEnvironment></p:environment>
  	<p:food>
  		<p:BiomassPS moduleName="BiomassPS">
  			<p:shelf cropType="WHEAT" cropArea="500" /><p:powerConsumer
  				inputs="PowerStore" desiredFlowRates="1000" maxFlowRates="1000" ></p:powerConsumer><p:potableWaterConsumer
  				inputs="PotableWaterStore" desiredFlowRates="3" maxFlowRates="3" ></p:potableWaterConsumer><p:greyWaterConsumer
  				inputs="GreyWaterStore" desiredFlowRates="3" maxFlowRates="3" /><p:dirtyWaterProducer
  				desiredFlowRates="2" outputs="DirtyWaterStore" maxFlowRates="2" ></p:dirtyWaterProducer><p:airConsumer
  				inputs="SimEnvironment" desiredFlowRates="1000" maxFlowRates="1000" /><p:airProducer
  				desiredFlowRates="1000" outputs="SimEnvironment" maxFlowRates="1000" /></p:BiomassPS>
  		<p:FoodStore capacity="10000000000" level="10000000000" moduleName="FoodStore"></p:FoodStore></p:food>
  	<p:power>
  		<p:PowerStore capacity="10000000" level="10000000" moduleName="PowerStore"></p:PowerStore></p:power>
  	<p:waste>
  		<p:DryWasteStore capacity="10000000" level="10000000" moduleName="DryWasteStore"></p:DryWasteStore></p:waste>
  	<p:water>
  		<p:PotableWaterStore capacity="10000000" level="10000000"
  			moduleName="PotableWaterStore">
  		</p:PotableWaterStore>
  		<p:GreyWaterStore capacity="10000000" level="10000000" moduleName="GreyWaterStore"></p:GreyWaterStore>
  		<p:DirtyWaterStore capacity="10000000" level="10000000" moduleName="DirtyWaterStore"></p:DirtyWaterStore></p:water>
  	<p:framework>
  		<p:Injector moduleName="O2Injector">
  			<p:O2Consumer inputs="O2Store" desiredFlowRates="100"
  				maxFlowRates="100" /><p:O2Producer desiredFlowRates="1000"
  				outputs="SimEnvironment" maxFlowRates="1000" /></p:Injector>
  		<p:Injector moduleName="CO2Injector">
  			<p:CO2Consumer inputs="CO2Store" desiredFlowRates="100"
  				maxFlowRates="100" /><p:CO2Producer desiredFlowRates="1000"
  				outputs="SimEnvironment" maxFlowRates="1000" /></p:Injector></p:framework>
  </p:SimBioModules>
	<p:Sensors>
		<p:environment>
			<p:GasConcentrationSensor input="SimEnvironment" moduleName="O2ConcentrationSensor"
				gasType="O2">
			</p:GasConcentrationSensor>
			<p:GasConcentrationSensor input="SimEnvironment" moduleName="CO2ConcentrationSensor"
				gasType="CO2">
			</p:GasConcentrationSensor></p:environment></p:Sensors>
	<p:Actuators>
		<p:air>
			<p:CO2InFlowRateActuator output="Injector" moduleName="InjectorCO2InFlowRateActuator"
				index="0">
			</p:CO2InFlowRateActuator>
			<p:O2InFlowRateActuator output="Injector" moduleName="InjectorO2InFlowRateActuator" index="0"></p:O2InFlowRateActuator></p:air></p:Actuators>
</p:biosim>
