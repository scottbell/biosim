<?xml version="1.0"?>
<biosim xmlns="http://www.traclabs.com/biosim" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.traclabs.com/biosim ../../schema/BiosimInitSchema.xsd">
		<Globals crewsToWatch="BaseCrewGroup" runTillCrewDeath="true" startPaused="false">
		<comment>
			Crew and plants, that's it.
		</comment>
		<author>Scott Bell</author>
		<!-- <log4jProperty name="log4j.logger.com.traclabs.biosim.server.sensor" value="DEBUG"/> -->
	</Globals>
	<SimBioModules>
		<crew>
			<CrewGroup moduleName="CrewGroup">
				<potableWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="PotableWaterStore"/>
				<airConsumer maxFlowRates="0" desiredFlowRates="0" inputs="Environment"/>
				<foodConsumer maxFlowRates="100" desiredFlowRates="100" inputs="FoodStore"/>
				<dirtyWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="DirtyWaterStore"/>
				<greyWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="GreyWaterStore"/>
				<airProducer maxFlowRates="0" desiredFlowRates="0" outputs="Environment"/>
				<dryWasteProducer maxFlowRates="100" desiredFlowRates="100" outputs="DryWasteStore"/>
				<crewPerson name="Bob Roberts" age="35" weight="70" sex="MALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
				<crewPerson name="Stephanie Stevens" age="32" weight="71" sex="FEMALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
				<crewPerson name="Bill Williams" age="30" weight="72" sex="MALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
				<crewPerson name="Janet Janey" age="34" weight="75" sex="FEMALE">
					<schedule>
						<activity name="sleep" length="8" intensity="1"/>
						<activity name="hygiene" length="1" intensity="2"/>
						<activity name="exercise" length="1" intensity="5"/>
						<activity name="eating" length="1" intensity="2"/>
						<activity name="mission" length="9" intensity="3"/>
						<activity name="health" length="1" intensity="2"/>
						<activity name="maintenance" length="1" intensity="2"/>
						<activity name="leisure" length="2" intensity="2"/>
					</schedule>
				</crewPerson>
			</CrewGroup>
		</crew>
		<environment>
			<SimEnvironment moduleName="Environment" initialVolume="2000000000"/>
		</environment>                    
		<food>
			<BiomassStore moduleName="BiomassStore" capacity="10000" level="0"/>
			<FoodStore moduleName="FoodStore" capacity="2000000" level="2000000"/>
			<BiomassPS moduleName="BiomassPS">
				<shelf cropArea="20" cropType="WHEAT"/>
				<shelf cropArea="20" cropType="TOMATO"/>
				<powerConsumer maxFlowRates="400" desiredFlowRates="400" inputs="PowerStore"/>
				<potableWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="PotableWaterStore"/>
				<greyWaterConsumer maxFlowRates="100" desiredFlowRates="100" inputs="GreyWaterStore"/>
				<airConsumer maxFlowRates="0" desiredFlowRates="0" inputs="Environment"/>
				<dirtyWaterProducer maxFlowRates="100" desiredFlowRates="100" outputs="DirtyWaterStore"/>
				<biomassProducer maxFlowRates="100" desiredFlowRates="100" outputs="BiomassStore"/>
				<airProducer maxFlowRates="0" desiredFlowRates="0" outputs="Environment"/>
			</BiomassPS>
		</food>
		<power>
			<PowerStore moduleName="PowerStore" capacity="50000000" level="50000000"/>
		</power>
		<water>
			<PotableWaterStore moduleName="PotableWaterStore" capacity="8000000" level="8000000"/>
			<GreyWaterStore moduleName="GreyWaterStore" capacity="40000" level="0"/>
			<DirtyWaterStore moduleName="DirtyWaterStore" capacity="50000" level="0"/>
		</water>
		<waste>
			<DryWasteStore moduleName="DryWasteStore" capacity="5000" level="0"/>
		</waste>
	</SimBioModules>
	<Sensors>
		<air>
			<O2InFlowRateSensor index="0" input="CrewGroup" moduleName="CrewGroupO2Sensor"/>
			<CO2OutFlowRateSensor index="0" input="CrewGroup" moduleName="CrewGroupCO2Sensor"/>
			<O2OutFlowRateSensor index="0" input="BiomassPS" moduleName="BiomassO2Sensor"/>
			<CO2InFlowRateSensor index="0" input="BiomassPS" moduleName="BiomassCO2Sensor"/>
		</air>
	</Sensors>
</biosim>

